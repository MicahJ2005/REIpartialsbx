<apex:page controller="VendorPortalAccountProfileController"  sidebar="false" cache="false" id="pg"  docType="html-5.0" showHeader="false">
    <apex:slds />
    <apex:composition template="CommunitiesTemplate">
        <apex:define name="header">
            <c:VendorPortalHeader ariaSelHome="false" ariaSelWO="true" ariaSelInvoice="false" ariaSelHWO="false" />
        </apex:define>
        <apex:define name="body">
            <html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                <head>
                    <style>
                        .textAreaCls{padding: 5px 1rem 0px 0.75rem !important;}
                        .slds-scope:not(html), .slds-scope body {
                        font-size: 1.3rem !important;                        
                        }
                        .buttonCls {    
                        height: 2.8rem !important;
                        font-size: 1.2rem !important;
                        }
                        .helpTextCls {
                        font-size: 12px !important;
                        }
                        .sectionCls {
                        width: 100%;
                        line-height: 2.875rem !important;
                        text-align: left !important;
                        text-transform: uppercase;
                        font-size: 1.2rem !important;
                        } 
                        .sectionCls1 {
                        width: 100%;
                        line-height: 2.875rem !important;
                        text-align: left !important;
                        font-size: 1.2rem !important;
                        } 
                        .slds-scope .slds-table {                    
                        font-size: small !important;
                        }
                        .slds-scope .slds-input {                       
                        min-height: calc(2.875rem + (1px * 2)) !important;
                        font: 400 13.3333px Arial !important;
                        }
                        .slds-scope .slds-form-element__label {                    
                        font-size: 1.2rem !important;
                        /*  font: 400 13.3333px Arial !important; */                   
                        }
                        .bPageBlock {
                        border-top: 4px solid #1b5297;
                        }
                        .slds-scope .slds-button+.slds-button {
                        margin-left: 0px;
                        }
                        .bPageBlock .requiredInput .requiredBlock {display: none;}
                        .addressBoxCls {
                        background-color: lightgrey;
                        width: -1px;
                        border: 1px solid lightgrey;
                        padding: 15px;
                        margin: 10px;        
                        }
                        .required-symbol{
                        border-left:2px solid  red !important;
                        } 
                        .requiredInput a{
                        border:none !important;
                        }
                        .datePicker{
                        z-index: 99999 ;
                        }
                        .dateFormat {
                        padding: 0 2px;
                        display: none;
                        }
                        .outputbackgColorCls{background-color: rgb(245,245,245) !important;}
                        .slds-scope .slds-p-horizontal--small {
                        padding-left: 1.50rem;
                        padding-right: .75rem;
                        }
                        .Custom32Tab .secondaryPalette, .individualPalette .Custom32Block .secondaryPalette {
                        background-color: #e5c130;
                        border-color: #1b5297;
                        }
                        .custom-file-upload {
                        border: 1px solid #ccc;
                        display: inline-block;
                        padding: 6px 12px;
                        cursor: pointer;
                        border-radius: 6px;
                        background: #1161a5;
                        color: #fff;
                        }   
                        a:focus,a:active,a:hover {
                        color:#2a94d6 !important;
                        }             
                    </style>
                </head>
                <body onload ="disableinput();">
                    
                    
                    <apex:form id="fromid"  >
                        <apex:actionStatus id="sts" style="align:center;">
                            <apex:facet name="start">
                                <div style="position: fixed; top: 0; left: 0; right: 0; bottom: 0; opacity: 0.25; z-index: 1000; background-color: black;">
                                    &nbsp;
                                </div>
                                <div style="position: fixed; left: 0; top: 0; bottom: 0; right: 0; z-index: 1001; margin: 15% 50%">
                                    <div style="display: inline-block; padding: 2px; background-color: #fff; width: 125px;">
                                        <img src="/img/loading.gif" style="float: left; margin: 8px;" />
                                        <span style="display: inline-block; padding: 10px 0px;">Please Wait...</span>
                                    </div>
                                </div>
                            </apex:facet>
                        </apex:actionStatus>
                        <div class="slds-spinner_container" id="spinner" style="display:none;position: fixed;">
                            <div class="slds-spinner--brand slds-spinner slds-spinner--medium" aria-hidden="false" role="alert" style="    position: fixed;">
                                <div class="slds-spinner__dot-a"></div>
                                <div class="slds-spinner__dot-b"></div>
                            </div>
                        </div>
                        <br/>                             
                        <div class="slds-col slds-box col2" style=" box-shadow: 1px 1px 25px rgba(0, 0, 0, 0.35);max-width: 90%;margin: 0px auto;background-color: white;" >
                            <center>
                                <b>
                                    <h1 style="font-size: x-large;">{!langconvertMap['VENDOR PROFILE']}</h1>
                                </b>
                            </center>
                            <br/>
                            <b style="color:red;font-size: 12px;">{!langconvertMap['NOTE:']} </b> <i style="color:red;font-size: 12px">{!langconvertMap['Please make sure you have saved your progress by clicking the ‘Save’ button before moving to another tab or leaving the site.']}</i>
                            <br/>
                            <b>
                                <i style="font-size:small;color:red;">
                                    <apex:outputText value="{!langconvertMap['Changes made are Waiting for Approval.']}" rendered="{!wrapperObj.accWrap.Approval_Status__c == 'Waiting for Approval'}"></apex:outputText>
                                </i>
                            </b>
                            <apex:outputPanel id="corporation">
                                <br/>       
                                <table width="100%"  >
                                    <tr width="100%">
                                        <td width="70%" valign="top" >
                                        </td>
                                        <td width="30%" style="text-align: right;" valign="top">
                                            <apex:commandlink action="{!EnglishLangChange}" value="English" immediate="true" styleClass="buttonCls"/>
                                            &nbsp;&nbsp;
                                            <apex:commandlink action="{!SpanishLangChange}" value="Spanish" immediate="true" styleClass="buttonCls"/>
                                        </td>
                                    </tr>
                                </table>
                                 <hr/>
                            <apex:pagemessages id="msg" />
                                        <center>
                                <apex:commandLink rendered="{!wrapperObj.accWrap.Approval_Status__c == 'In Process' || wrapperObj.accWrap.Approval_Status__c == ''}" action="{!doUpdateVendorProfile}" onclick="$('[id$=spinner]').css('display','block');" oncomplete="$('[id$=spinner]').css('display','none');" reRender="msg">
                                    <button class="slds-button slds-button_brand buttonCls " >{!langconvertMap['Save']}</button>
                                </apex:commandLink>
                                <apex:commandLink action="{!doCancel}" >
                                    <button class="slds-button slds-button_destructive buttonCls " >{!langconvertMap['Cancel']}</button>
                                </apex:commandLink>
                                <apex:commandLink action="{!doSubmitVendorProfile}" onclick="$('[id$=spinner]').css('display','block');" oncomplete="$('[id$=spinner]').css('display','none');" reRender="script-block, msg">
                                    <button class="slds-button slds-button_success buttonCls " >{!langconvertMap['Submit']}</button>               
                                </apex:commandLink>
                            </center>
                                <br/>
                                <div class="slds-grid slds-gutters_small">
                                    <div class="slds-col slds-has-flexi-truncate slds-grid" role="listitem">
                                        <fieldset class="slds-form-element slds-form_compound slds-is-required slds-is-editing slds-form-element_stacked">
                                            <legend class="slds-form-element__legend slds-form-element__label"></legend>
                                            <div class="slds-form-element__control">
                                                <div class="slds-form-element__group">
                                                    <div class="slds-form-element__row">
                                                        <div class="slds-form-element slds-size_1-of-1">
                                                            <label class="slds-form-element__label" for="form-element-id-04">{!langconvertMap['Legal Business Name']}:</label>
                                                            <div class="mouseOverInfoOuter" id="searchInvoiceHelper" onfocus="addMouseOver(this)" onmouseover="addMouseOver(this)" tabindex="0">
                                                                <img src="/s.gif" alt="" class="infoIcon" title="" />
                                                                <div class="mouseOverInfo" id="searchInvoiceHelperText" style="display: none; opacity: -0.19999999999999996; left: 16px;">
                                                                    <div class="body helpTextCls" >
                                                                        {!langconvertMap['Name as shown on your income tax return and on Line 1 of your W9.']}
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="slds-form-element__control">
                                                                <apex:inputfield value="{!wrapperObj.accWrap.Name}" html-autocomplete="off" styleClass="slds-input required-symbol " rendered="{!AND(wrapperObj.accWrap.Approval_Status__c != 'Approved', wrapperObj.accWrap.Approval_Status__c != 'Waiting for Approval')}"/>
                                                                <apex:outputfield value="{!wrapperObj.accWrap.Name}" html-autocomplete="off" styleClass="slds-input" rendered="{!OR(wrapperObj.accWrap.Approval_Status__c == 'Approved', wrapperObj.accWrap.Approval_Status__c == 'Waiting for Approval')}"/>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                    <div class="slds-col slds-has-flexi-truncate slds-grid" role="listitem">
                                        <fieldset class="slds-form-element slds-form_compound slds-is-required slds-is-editing slds-form-element_stacked">
                                            <legend class="slds-form-element__legend slds-form-element__label"></legend>
                                            <div class="slds-form-element__control">
                                                <div class="slds-form-element__group">
                                                    <div class="slds-form-element__row">
                                                        <div class="slds-form-element slds-size_1-of-1">
                                                            <label class="slds-form-element__label" for="form-element-id-04">{!langconvertMap['Doing Business As (DBA) Name']}:</label>
                                                            <div class="mouseOverInfoOuter" id="searchInvoiceHelper" onfocus="addMouseOver(this)" onmouseover="addMouseOver(this)" tabindex="0">
                                                                <img src="/s.gif" alt="" class="infoIcon" title="" />
                                                                <div class="mouseOverInfo" id="searchInvoiceHelperText" style="display: none; opacity: -0.19999999999999996; left: 16px;">
                                                                    <div class="body helpTextCls">
                                                                        {!langconvertMap['Name under which you operate your business, if different from the Legal Business Name, and as shown on Line 2 of your W9.']}
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="slds-form-element__control">
                                                                <apex:inputfield value="{!wrapperObj.accWrap.Business_Name__c}" html-autocomplete="off" styleClass="slds-input" rendered="{!AND(wrapperObj.accWrap.Approval_Status__c != 'Approved', wrapperObj.accWrap.Approval_Status__c != 'Waiting for Approval')}"/>
                                                                <apex:outputfield value="{!wrapperObj.accWrap.Business_Name__c}" html-autocomplete="off" styleClass="slds-input" rendered="{!OR(wrapperObj.accWrap.Approval_Status__c == 'Approved', wrapperObj.accWrap.Approval_Status__c == 'Waiting for Approval')}"/>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                    <div class="slds-col slds-has-flexi-truncate slds-grid" role="listitem">
                                        <fieldset class="slds-form-element slds-form_compound slds-is-editing slds-form-element_stacked">
                                            <legend class="slds-form-element__legend slds-form-element__label"></legend>
                                            <div class="slds-form-element__control">
                                                <div class="slds-form-element__group">
                                                    <div class="slds-form-element__row">
                                                        <div class="slds-form-element slds-size_1-of-1">
                                                            <label class="slds-form-element__label" for="form-element-id-04">{!langconvertMap['Taxpayer Identification Number (TIN)']}:</label>
                                                            <div class="mouseOverInfoOuter" id="searchInvoiceHelper" onfocus="addMouseOver(this)" onmouseover="addMouseOver(this)" tabindex="0">
                                                                <img src="/s.gif" alt="" class="infoIcon" title="" />
                                                                <div class="mouseOverInfo" id="searchInvoiceHelperText" style="display: none; opacity: -0.19999999999999996; left: 16px;">
                                                                    <div class="body helpTextCls">
                                                                        {!langconvertMap['The TIN provided must match the Legal Business Name given and the Taxpayer Identification Number as shown in Part 1 of your W-9.']}
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="slds-form-element__control">
                                                                <apex:inputText value="{!tinNum}" html-autocomplete="off" styleClass="slds-input required-symbol" rendered="{!AND(wrapperObj.accWrap.Approval_Status__c != 'Approved', wrapperObj.accWrap.Approval_Status__c != 'Waiting for Approval')}"/>
                                                                <apex:outputText value="{!tinNum}" html-autocomplete="off" styleClass="slds-output" rendered="{!OR(wrapperObj.accWrap.Approval_Status__c == 'Approved', wrapperObj.accWrap.Approval_Status__c == 'Waiting for Approval')}"/>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                </div>
                                <div class="slds-grid slds-gutters_small">
                                    <div class="slds-col slds-has-flexi-truncate slds-grid" role="listitem">
                                        <fieldset class="slds-form-element slds-form_compound slds-is-required slds-is-editing slds-form-element_stacked">
                                            <legend class="slds-form-element__legend slds-form-element__label"></legend>
                                            <div class="slds-form-element__control">
                                                <div class="slds-form-element__group">
                                                    <div class="slds-form-element__row">
                                                        <div class="slds-form-element slds-size_1-of-2">
                                                            <label class="slds-form-element__label" for="bPhone">{!langconvertMap['Phone']}:</label>
                                                            <div class="slds-form-element__control">
                                                                <apex:inputfield id="bPhone" value="{!wrapperObj.accWrap.Phone}" html-autocomplete="off" onchange="phonenumber(this)" styleClass="slds-input" />
                                                            </div>
                                                        </div>
                                                        <div class="slds-form-element slds-size_1-of-2">
                                                            <label class="slds-form-element__label" for="bEmail">{!langconvertMap['Email']}:</label>
                                                            <div class="slds-form-element__control">
                                                                <apex:inputfield id="bEmail" value="{!wrapperObj.accWrap.Vendor_Email__c}" html-autocomplete="off" styleClass="slds-input" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                </div>
                                <div class="slds-grid slds-gutters_small">
                                    <div class="slds-col slds-has-flexi-truncate slds-grid" role="listitem">
                                        <fieldset class="slds-form-element slds-form_compound slds-is-editing slds-form-element_stacked">
                                            <legend class="slds-form-element__legend slds-form-element__label"></legend>
                                            <div class="slds-form-element__control">
                                                <div class="slds-form-element__group">
                                                    <div class="slds-form-element__row">
                                                        <div class="slds-form-element slds-size_1-of-1">
                                                            <label class="slds-form-element__label" for="shipStreet">{!langconvertMap['Street']}:</label>
                                                            <div class="slds-form-element__control">
                                                                <apex:inputfield id="shipStreet" value="{!wrapperObj.accWrap.ShippingStreet}" html-autocomplete="off" styleClass="slds-input textAreaCls"/>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="slds-form-element__row">
                                                        <div class="slds-form-element slds-size_1-of-3">
                                                            <label class="slds-form-element__label" for="shipCity">{!langconvertMap['City']}:</label>
                                                            <div class="slds-form-element__control">
                                                                <apex:inputfield id="shipCity" value="{!wrapperObj.accWrap.ShippingCity}" html-autocomplete="off" styleClass="slds-input"/>
                                                            </div>
                                                        </div>
                                                        <div class="slds-form-element slds-size_1-of-3">
                                                            <label class="slds-form-element__label" for="shipState">{!langconvertMap['State']}:</label>
                                                            <div class="slds-form-element__control">
                                                                <apex:inputfield id="shipState" value="{!wrapperObj.accWrap.ShippingState}" html-autocomplete="off" onkeyup="this.value = this.value.toUpperCase();" html-maxlength="2" styleClass="slds-input"  />
                                                            </div>
                                                        </div>
                                                        <div class="slds-form-element slds-size_1-of-3">
                                                            <label class="slds-form-element__label" for="shipZip">{!langconvertMap['Zip Code']}:</label>
                                                            <div class="slds-form-element__control">
                                                                <apex:inputfield id="shipZip" value="{!wrapperObj.accWrap.ShippingPostalCode}" html-autocomplete="off" styleClass="slds-input"/>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                </div>
                                <apex:outputPanel id="categoryPanel">
                                    <div class="slds-grid slds-gutters_small">
                                        <div class="slds-col slds-has-flexi-truncate slds-grid" role="listitem">
                                            <fieldset class="slds-form-element slds-form_compound slds-is-required slds-is-editing slds-form-element_stacked">
                                                <legend class="slds-form-element__legend slds-form-element__label"></legend>
                                                <div class="slds-form-element__control">
                                                    <div class="slds-form-element__group">
                                                        <div class="slds-form-element__row">
                                                            <div class="slds-form-element slds-size_1-of-2">
                                                                <label class="slds-form-element__label" for="web">{!langconvertMap['Website']}:</label>
                                                                <div class="slds-form-element__control">
                                                                    <apex:inputfield id="web" value="{!wrapperObj.accWrap.Website}" html-autocomplete="off" styleClass="slds-input" />
                                                                </div>
                                                                <br/>
                                                                <label class="slds-form-element__label" for="type">{!langconvertMap['Type']}:</label>
                                                                <div class="slds-form-element__control">
                                                                    <apex:inputfield id="type" value="{!wrapperObj.accWrap.Type}" html-autocomplete="off"  styleClass="slds-input" >
                                                                        <apex:actionSupport event="onchange" action="{!callMethodNew}" onsubmit="$('[id$=spinner]').css('display','block');" oncomplete="$('[id$=spinner]').css('display','none');" reRender="pg:CommunitiesTemplate:fromid:categoryPanel, FileList"/>
                                                                    </apex:inputfield>
                                                                </div>
                                                                <br/>
                                                                <i>
                                                                    <apex:outputText style="font-size: 12px;" value="{!langconvertMap['Rehab & Maintenance Vendors will be interviewed and approved by REI Nation & PPMG.']} {!langconvertMap['Do not declare new skillsets without first discussing with the proper Management teams as certain categories may require training, certifications and insurance.']}" />
                                                                </i>
                                                            </div>
                                                            <div class="slds-form-element slds-size_1-of-2">
                                                                <label class="slds-form-element__label" for="form-element-id-04">
                                                                    {!langconvertMap['Markets']}: 
                                                                    <apex:outputText style="font-weight:bold;" id="mkt" value="{!marketString}"/>
                                                                </label>
                                                                <div class="slds-form-element__control">
                                                                    <div Class="slds-input new_input" style="overflow-y: scroll;height:180px;">
                                                                        <apex:repeat value="{!marketWrapList}" var="wrapper" >
                                                                            <apex:inputCheckbox value="{!wrapper.isSelected}" disabled="{!wrapperObj.accWrap.Approval_Status__c == 'Waiting for Approval'}" >
                                                                                <apex:actionSupport event="onchange" action="{!marketValues}" rerender="mkt, FileList" onsubmit="$('[id$=spinner]').css('display','block');" oncomplete="$('[id$=spinner]').css('display','none');"/>
                                                                            </apex:inputCheckbox>
                                                                            {!wrapper.label}<br/> 
                                                                        </apex:repeat>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="slds-form-element__row">
                                                            <div class="slds-form-element slds-size_1-of-2">
                                                                <label class="slds-form-element__label" for="cat">
                                                                    {!langconvertMap['Rehab Categories']}: 
                                                                    <apex:outputText style="font-weight:bold;" id="rehabCat" value="{!rehabCatString}"/>
                                                                </label>
                                                                <div class="slds-form-element__control">
                                                                    <div class="slds-input new_input" style="overflow-y: scroll;height:180px;">
                                                                        <apex:outputPanel id="rehab">
                                                                            <apex:repeat value="{!lst_rehabContractorTrades}" var="rehabWrap">
                                                                                <apex:inputCheckbox id="cat" value="{!rehabWrap.isSelected}" disabled="{!OR(wrapperObj.accWrap.Type == 'Maintenance',wrapperObj.accWrap.Approval_Status__c == 'Waiting for Approval')}" >
                                                                                    <apex:actionSupport event="onchange" action="{!rehabCatValues}" rerender="rehabCat, FileList" onsubmit="$('[id$=spinner]').css('display','block');" oncomplete="$('[id$=spinner]').css('display','none');"/>
                                                                                </apex:inputCheckbox>
                                                                                {!rehabWrap.label}<br/> 
                                                                            </apex:repeat>
                                                                        </apex:outputPanel>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="slds-form-element slds-size_1-of-2">
                                                                <label class="slds-form-element__label" for="mCat">
                                                                    {!langconvertMap['Maintenance Categories']}: 
                                                                    <apex:outputText style="font-weight:bold;" id="maintCat" value="{!maintCatString}"/>
                                                                </label>
                                                                <div class="slds-form-element__control">
                                                                    <div Class="slds-input new_input" style="overflow-y: scroll;height:180px;">
                                                                        <apex:outputPanel id="maintenance">
                                                                            <apex:repeat value="{!lst_maintenanceProviderTrades}" var="maintWrap">
                                                                                <apex:inputCheckbox id="mCat" value="{!maintWrap.isSelected}" disabled="{!OR(wrapperObj.accWrap.Type == 'Rehab', wrapperObj.accWrap.Approval_Status__c == 'Waiting for Approval')}" >
                                                                                    <apex:actionSupport event="onchange" action="{!maintCatValues}" rerender="maintCat, FileList" onsubmit="$('[id$=spinner]').css('display','block');" oncomplete="$('[id$=spinner]').css('display','none');"/>
                                                                                </apex:inputCheckbox>
                                                                                {!maintWrap.label}<br/> 
                                                                            </apex:repeat>
                                                                        </apex:outputPanel>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="slds-form-element__row">
                                                            <div class="slds-form-element slds-size_1-of-2">
                                                                <label class="slds-form-element__label" for="license">
                                                                    {!langconvertMap['License #']}
                                                                </label>
																<div class="mouseOverInfoOuter" id="searchInvoiceHelper" onfocus="addMouseOver(this)" onmouseover="addMouseOver(this)" tabindex="0">
                                                                <img src="/s.gif" alt="" class="infoIcon" title="" />
                                                                <div class="mouseOverInfo" id="searchInvoiceHelperText" style="display: none; opacity: -0.19999999999999996; left: 16px;">
                                                                    <div class="body helpTextCls">
                                                                        {!langconvertMap['Professional / Trade License Number']}
                                                                    </div>
                                                                </div>
                                                            </div>
                                                                <div class="slds-form-element__control">
                                                                    <apex:inputField value="{!wrapperObj.accWrap.License__c}" html-autocomplete="off" styleClass="slds-input"/>
                                                                </div>
                                                            </div>
                                                            <div class="slds-form-element slds-size_1-of-2">
                                                            </div>
                                                        </div>
                                                        
                                                    </div>
                                                </div>
                                            </fieldset>
