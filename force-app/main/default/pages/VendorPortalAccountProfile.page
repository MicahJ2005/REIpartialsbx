                    if('{!error}' != '' && '{!error}' != null && '{!error}' != 'undefined'){
                        showError('{!error}');
                    }
                    </script>
                </apex:outputPanel>
                <script>
                function phonenumber(inputtxt) {
                    var phoneno = /^\(?([0-9]{3})\)?[-. ]?([0-9]{3})[-. ]?([0-9]{4})$/;
                    if(inputtxt.value.match(phoneno)) {
                        return true;
                    }
                    else {
                        inputtxt.value = '';
                        alert("Phone number format must be XXX-XXX-XXXX");
                        return false;
                    }
                }
                function openModal(){
                    $('[id$=spinner]').css('display','none');
                    $(".legalAck.slds-modal").addClass('slds-fade-in-open');
                    $(".slds-backdrop").addClass('slds-backdrop--open');
                }
                
                function closeModal(){
                    $(".legalAck.slds-modal").removeClass('slds-fade-in-open');
                    $(".slds-backdrop").removeClass('slds-backdrop--open');
                }
                
                function showError(error) {
                    $(".slds-notify_container h2").each(function() {
                        $(this).text(error);
                    });
                    $(".slds-notify_container").removeClass('slds-hide');
                    setTimeout(function() {
                        $(".slds-notify_container").addClass('slds-hide');
                    }, 8000);
                }
                // ******* Attachment Script Start ******
                
                var blobfile;
                var conDocId;
                var attType;
                var cvId;
                var expDtDL = null;
                var expDtGL = null;
                var expDtWC = null;
                var expDtLicense = null;
                var docTypeToAttachIdMap = new Map([
                    ["License", ""], 
                    ["Driver’s License/Photo Id" ,"" ],
                    ["Certificate of Insurance - General Liability" ,"" ],
                    ["Certificate of Insurance or Waiver - Workers Comp" ,"" ]
                ]);
                
                function uploadFile(cvId, cdId, attachType){
                    
                    var fbody= document.getElementById("fileId"+attachType).files[0];
                    cvId = cvId;
                    conDocId = cdId;
                    attType = attachType;
                    getAsText(fbody);
                    
                }
                function getAsText(readFile) {
                    
                    var reader = new FileReader();
                    if(readFile == undefined){
                        showError(' Please input a valid file. ');
                        $('[id$=spinner]').css('display','none');
                        return;
                    }else{
                        reader.readAsDataURL(readFile);
                        reader.onload = attLoadedNew;
                    }
                    
                }
                
                //verify the file extension, throw error if other than given values below
                // creates attachment
                
                function attLoadedNew(evt) {  
                    sforce.connection.serverUrl = '{!$Site.Prefix}/services/Soap/u/40.0';
                    sforce.connection.sessionId = '{!$Api.Session_ID}';
                    console.log(evt.target);
                    var fileString = evt.target.result;
                    var extension;
                    var blobfile = fileString;
                    var input = document.getElementById("fileId"+attType);
                    
                    filename= input.value;
                    console.log('filename------'+filename);
                    extension = filename.substring(filename.lastIndexOf(".") + 1, filename.length).toLowerCase();
                    console.log('extension------'+extension);
                    
                    //var attachmentTypeVal = document.getElementById('pg:CommunitiesTemplate:fromid:attachmentType').value; 
                    var attachmentTypeVal = attType;                                                      
                    //var expDt= document.getElementById('pg:CommunitiesTemplate:fromid:expDt').value; 
                    var element = document.getElementsByClassName("licen");
                    if (element[0] != null && element[0] != undefined) {
                        expDtLicense= element[0].value;
                    }
                    element = document.getElementsByClassName("dl");                         
                    if (element[0] != null && element[0] != undefined) {
                        expDtDL= element[0].value;
                    }
                    element = document.getElementsByClassName("gl");
                    if (element[0] != null && element[0] != undefined) {
                        expDtGL= element[0].value;
                    }
                    element = document.getElementsByClassName("wc");
                    console.log('element------'+element);
                    if (element[0] != null && element[0] != undefined) {
                        expDtWC = element[0].value;
                    }   
                    console.log('attachmentTypeVal------'+attachmentTypeVal);
                    console.log('expDtGL------'+expDtGL);
                    
                    if(extension != 'pdf' && extension != 'xlsx' &&
                       extension != 'xls' && extension !='csv' && 
                       extension !='doc' && extension != 'docx' &&
                       extension !='ppt' && extension !='pptx' && extension !='png' &&
                       extension != 'jpg' && extension != 'jpeg' && extension != 'jfif'){
                        stopSpinner();
                        showError('Attachments can only be in xlsx, xls, csv, doc, docx, pdf, ppt, pptx, png, and jpg formats.');
                        
                    }
                    else{
                        console.log('conDocId------'+conDocId);
                        var attach = new sforce.SObject('ContentVersion');
                        attach.origin       = 'H'; 
                        if(cvId != null && cvId != ''){
                            attach.Id = cvId;
                        }
                        if(conDocId != null && conDocId != ''){
                            attach.ContentDocumentId = conDocId;
                        }
                        else{                                    
                            attach.FirstPublishLocationId= '{!wrapperObj.accWrap.Id}';                          
                        }
                        
                        attach.Title= filename.replace(/.*[\/\\]/, '');
                        attach.PathOnClient= filename.replace(/.*[\/\\]/, '');
                        attach.VersionData= blobfile.split(',')[1];
                        attach.Status__c = 'Uploaded';
                        
                        if(attachmentTypeVal == "License" && expDtLicense != null && expDtLicense != ''){
                            attach.Expiration_Date__c = expDtLicense;
                            console.log('attach.Expiration_Date__c-----'+attach.Expiration_Date__c);
                        }
                        if(attachmentTypeVal == "Driver’s License/Photo Id" && expDtDL != null && expDtDL != ''){
                            attach.Expiration_Date__c = expDtDL;
                            console.log('attach.Expiration_Date__c-----'+attach.Expiration_Date__c);
                        }
                        else if(attachmentTypeVal == "Certificate of Insurance - General Liability" && expDtGL != null && expDtGL != ''){
                            attach.Expiration_Date__c = expDtGL;
                        } 
                            else if(attachmentTypeVal == "Certificate of Insurance or Waiver - Workers Comp" && expDtWC != null && expDtWC != ''){
                                attach.Expiration_Date__c = expDtWC;
                            } 
                        attach.Attachment_Type__c = attachmentTypeVal;
                        var result = sforce.connection.upsert("Id",[attach],{onSuccess : success, onFailure : failure});
                    }
                }
                
                function addDateToAttachment(attachType) {  
                    sforce.connection.serverUrl = '{!$Site.Prefix}/services/Soap/u/40.0';
                    sforce.connection.sessionId = '{!$Api.Session_ID}';
                    attType = attachType;
                    
                    console.log("attType: " + attType);
                    console.log("expDtLicense BEFORE: " + expDtLicense);
                    var attachmetnId = getAttachmentId();
                    console.log("attachmetnId: " + attachmetnId);
                    
                    var attach = new sforce.SObject('ContentVersion');
                    if(attachmetnId != null && attachmetnId != ''){
                        attach.Id = attachmetnId;
                    }
                    
                    if(attType == "License" && expDtLicense != null && expDtLicense != ''){
                        attach.Expiration_Date__c = expDtLicense;
                    }
                    if(attType == "Driver’s License/Photo Id" && expDtDL != null && expDtDL != ''){
                        attach.Expiration_Date__c = expDtDL;
                    }
                    else if(attType == "Certificate of Insurance - General Liability" && expDtGL != null && expDtGL != ''){
                        attach.Expiration_Date__c = expDtGL;
                    } 
                    else if(attType == "Certificate of Insurance or Waiver - Workers Comp" && expDtWC != null && expDtWC != ''){
                        attach.Expiration_Date__c = expDtWC;
                    } 
                    
                    console.log(attach);
                    if(attach.Id != null && attach.Id != '') {
                        var result = sforce.connection.upsert("Id",[attach],{onSuccess : successAddDate, onFailure : failure});
                    }
                }
                
                function getAttachmentId() {
                    var element = document.getElementsByClassName("licen");
                    if (element[0] != null && element[0] != undefined) {
                        expDtLicense= element[0].value;
                    }
                    element = document.getElementsByClassName("dl");                         
                    if (element[0] != null && element[0] != undefined) {
                        expDtDL= element[0].value;
                    }
                    element = document.getElementsByClassName("gl");
                    if (element[0] != null && element[0] != undefined) {
                        expDtGL= element[0].value;
                    }
                    element = document.getElementsByClassName("wc");
                    console.log('element------'+element);
                    if (element[0] != null && element[0] != undefined) {
                        expDtWC = element[0].value;
                    }
                    
                    return docTypeToAttachIdMap.get(attType);
                }
                
                function success(result){
                    var relatedDateFieldClass;                    
                    error='';
                    var expDt = '';
                    console.log('attType-----'+attType);
                    
                    if(attType == "License"){
                        expDt = expDtLicense;
                        relatedDateFieldClass = 'slds-input licen';
                    }
                    else if(attType == "Driver’s License/Photo Id"){
                        expDt = expDtDL;
                        relatedDateFieldClass = 'slds-input dl';
                    }
                    else if(attType == "Certificate of Insurance - General Liability"){
                        expDt = expDtGL;
                        relatedDateFieldClass = 'slds-input gl';
                    } 
                    else if(attType == "Certificate of Insurance or Waiver - Workers Comp"){
                        expDt = expDtWC;
                        relatedDateFieldClass = 'slds-input wc';
                    }  
                    
                    if(expDt == null || expDt == ''){
                        expDt = null;
                    } 
                    console.log('result[0]------'+result[0]);      
                    console.log('expDt------'+expDt);
                    
                    if(relatedDateFieldClass != undefined) {
                        var dateFieldElems = document.getElementsByClassName(relatedDateFieldClass);
                        if(dateFieldElems.length > 0) {
                            docTypeToAttachIdMap.set(attType, result[0].id);
                        }
                    }
                    attachFileFunction(result[0].id, attType, expDt);
                }
                
                function successAddDate(result){
                    console.log('SUCCESS------');
                }
                    
                function failure(error){
                    console.log('error------'+error);
                    showError(error);
                }
                
                
                // ******* Attachment Script End ******
                
                $(document).ready(function(){
                    //$('.custom-file-upload').click(function(){ $('.fileIdBtn').trigger('click'); });
                });
                
                function stopSpinner(){
                    $('[id$=spinner]').css('display','none');
                }
                
                </script>
            </html>
